{
  "manifest_version": 2,
  "name": "WE Find Demo - Advanced",
  "description": "WE Find Demo - Advanced",
  "version": "1.0",

  "background": {
    "scripts": ["background.js"]
  },

  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content_script.js"]
    }
  ],

  "permissions": [
    "find",
    "activeTab",
    "tabs"
  ],

  "icons": {
    "32": "icons/button_close_red_12.png",
    "48": "icons/button_close_red_12.png"
  },

  "browser_action": {
    "default_icon": "icons/button_close_red_12.png",
    "default_title": "Find Demo Adv"
  }

}
